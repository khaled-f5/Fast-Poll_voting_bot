import time
from selenium import webdriver

for i in range(5000):
    for j in range(5000):
        start = time.time()
        driver = webdriver.Chrome('path to chromedriver.exe')
        driver.get('vote link')
        check = time.time() - start
        try:
            time.sleep(1)
            button_cookie = driver.find_element_by_id('gdpr-cookie-accept')
            button_cookie.click()

            button_select = driver.find_element_by_xpath('xpath for the voting button')
            button_select.click()

            driver.execute_script("window.scrollTo(0, 1000);")

            button_submit = driver.find_element_by_xpath('//*[@id="poll-form"]/div[15]/button')
            button_submit.click()
            driver.close()
            end = time.time() - start
            print('elapsed time:', int(end), 'sec', '\tvote number: ', j+1)
        except:
            print('failed')
            driver.close()
